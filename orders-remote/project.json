{
  "name": "orders-remote",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "netquest",
  "sourceRoot": "orders-remote/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/orders-remote",
        "index": "orders-remote/src/index.html",
        "main": "orders-remote/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "orders-remote/tsconfig.app.json",
        "assets": ["orders-remote/src/favicon.ico", "orders-remote/src/assets"],
        "styles": ["orders-remote/src/styles.css"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "orders-remote/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "orders-remote/webpack.prod.config.ts"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:webpack-dev-server",
      "options": {
        "port": 4204,
        "publicHost": "http://localhost:4204"
      },
      "configurations": {
        "production": {
          "buildTarget": "orders-remote:build:production"
        },
        "development": {
          "buildTarget": "orders-remote:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "orders-remote:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["orders-remote/**/*.ts", "orders-remote/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "orders-remote/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "orders-remote:build",
        "port": 4204,
        "watch": false
      },
      "configurations": {
        "development": {
          "buildTarget": "orders-remote:build:development"
        },
        "production": {
          "buildTarget": "orders-remote:build:production"
        }
      }
    }
  }
}
